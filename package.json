{
  "name": "projectdark",
  "author": "Anar Gasimov",
  "displayName": "ProjectDark",
  "description": "ProjectMark logger.service{} dimmer",
  "icon": "static/pm_logo.png",
  "repository": "https://github.com/artmontinski/projectdark",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectdark.showHideStatusBar",
        "title": "ProjectDark: Show/Hide Dimmer in Status Bar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rm -rf out && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
